<%= form_for @csv, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.file_field :csv %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                csvs_path, :class => 'btn' %>
  </div>
<% end %>
